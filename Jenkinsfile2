pipeline {
  agent any
  stages {
    stage('Example Build') {
      steps {
        sh 'echo Hello World'
      }
    }
    stage('Example Deploy') {
    
    if env.BRANCH_NAME == "dev" {
          
      steps {
        sh 'echo Deploying main'
      }
              }
 
    }
   stage('Examdple Deploy') {
      when {
        branch 'dev'
      }
      steps {
        sh 'echo Deploying dev '
      }
    }
  }
}
